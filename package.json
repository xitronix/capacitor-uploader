{
  "name": "@capgo/capacitor-uploader",
  "version": "7.1.0",
  "description": "Upload file natively",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Sources",
    "ios/Tests",
    "Package.swift",
    "CapgoCapacitorUploader.podspec"
  ],
  "author": "Martin Donadieu <martin@capgo.app>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cap-go/capacitor-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/Cap-go/capacitor-uploader/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "uploader",
    "s3",
    "upload",
    "file",
    "upload",
    "upload-file",
    "upload-files",
    "upload-file-to-s3",
    "upload-files-to-s3",
    "capgo",
    "capacitor-uploader",
    "background",
    "background-upload",
    "background-upload-file",
    "background-upload-files",
    "background-upload-file-to-s3",
    "background-upload-files-to-s3"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -sdk iphoneos -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --autocorrect --format",
    "eslint": "eslint .",
    "prettier": "prettier --config .prettierrc.js \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api UploaderPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {
    "idb": "^8.0.2"
  },
  "devDependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/cli": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/ios": "^7.0.1",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "eslint": "^9.21.0",
    "prettier": "^3.5.1",
    "prettier-plugin-java": "^2.6.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "swiftlint": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@capacitor/core": ">=7.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "no-empty": "off"
    }
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}